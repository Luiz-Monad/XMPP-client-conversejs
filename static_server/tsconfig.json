{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "outDir": "../dist/static_server",
        "declaration": true,
        "strict": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "skipLibCheck": true,
        "noImplicitThis": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true
    },
    "include": ["./*.ts"],
    "exclude": ["./node_modules/**"]
}
